{
  "version": 2,
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "^(?!/api/).*$", "dest": "/index.html" }
  ],
  "functions": {
    "api/[...all].ts": {
      "maxDuration": 30,
      "includeFiles": "server/**"
    },
    "api/debug/imports.ts": {
      "maxDuration": 10,
      "includeFiles": "server/**"
    }
  }
}
